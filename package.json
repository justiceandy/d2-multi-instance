{
  "name": "@d2r/launcher",
  "version": "1.0.0",
  "bin": "index.js",
  "private": "true",
  "description": "supports logging into multiple bnet accounts for diablo 2 multiboxing",
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna exec npm install --no-bail",
    "postbootstrap" : "lerna link"
   },
  "author": "",
  "license": "ISC",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/electron-webpack",
      "**/electron-webpack/**",
      "**/electron-builder/**",
      "**/electron-builder"
    ]
  },
  "engines": {
    "yarn": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
		"electron": "^15.3.0",
    "@babel/preset-env": "^7.16.4",
    "electron-builder": "^22.13.1",
    "lerna": "^4.0.0",
    "typescript": "^4.4.4"
  }
}
