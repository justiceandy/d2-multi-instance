{
    "presets": ["@babel/preset-env", "@babel/preset-typescript"],
    "plugins": [
      ["@emotion",
        {
          "autoLabel": "dev-only",
          "labelFormat": "[filename]--[local]",
          "cssPropOptimization": true
        }
      ],
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
