{
  "name": "@d2r/ui",
  "version": "1.0.0",
  "description": "D2R React UI Components",
  "types": "dist/index.js",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 6006",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js ",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "watch": "babel --watch src -d dist --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "postbuild2": "tsc -p ./tsconfig.json"
  },
